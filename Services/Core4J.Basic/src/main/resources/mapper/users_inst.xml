<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ikcoder.services.mappers.Mapper_users_inst">
    <select id="Select_userid" resultType="ikcoder.services.entities.DTO.DTO_users_inst">
            select * from users_inst where user_id=#{userid}
    </select>
    <select id="Select_users_instid" resultType="java.util.List">
            select * from users_inst where inst_id=#{inst_id}
    </select>
</mapper>